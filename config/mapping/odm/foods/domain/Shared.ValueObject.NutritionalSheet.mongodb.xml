<doctrine-mongo-mapping xmlns="http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping
                    http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping.xsd">
    <embedded-document name="App\Food\Domain\Shared\ValueObject\NutritionalSheet">
        <embed-one field="proteins" target-document="App\Food\Domain\Shared\ValueObject\NutritionalSheetValue" />
        <embed-one field="fats" target-document="App\Food\Domain\Shared\ValueObject\NutritionalSheetValue" />
        <embed-one field="carbs" target-document="App\Food\Domain\Shared\ValueObject\NutritionalSheetValue" />
        <embed-one field="calories" target-document="App\Food\Domain\Shared\ValueObject\NutritionalSheetValue" />
    </embedded-document>
</doctrine-mongo-mapping>